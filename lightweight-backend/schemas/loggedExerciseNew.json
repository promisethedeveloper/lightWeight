{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "http://example.com/example.json",
	"type": "object",
	"default": {},
	"title": "Root Schema",
	"required": ["weight", "unit", "noOfSets", "noOfReps"],
	"properties": {
		"workoutId": {
			"type": "integer",
			"minimum": 0
		},
		"exerciseId": {
			"type": "integer",
			"minimum": 0
		},
		"weight": {
			"type": "integer",
			"minimum": 0
		},
		"unit": {
			"type": "string",
			"minLength": 1,
			"maxLength": 30
		},
		"noOfSets": {
			"type": "integer",
			"minimum": 0
		},
		"noOfReps": {
			"type": "integer",
			"minimum": 0
		}
	}
}
